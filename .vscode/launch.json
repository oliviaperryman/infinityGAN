    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Test invert",
            "type": "python",
            "request": "launch",
            "program": "test.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "/local/omp/infinityGAN/src",
                "CUDA_VISIBLE_DEVICES": "0" 
            },
            "args": [
                "--model-config",
                "./configs/model/InfinityGAN-IOF.yaml",
                "--test-config",
                "./configs/test/inversion_256x256_L2R.yaml",
                // "./configs/test/inversion_IOF246_256x1280L_256x128.yaml",
                "--inv-start-idx",
                "2"
            ]
        },
        {
            "name": "Test outpaint",
            "type": "python",
            "request": "launch",
            "program": "test.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "/local/omp/infinityGAN/src",
                "CUDA_VISIBLE_DEVICES": "0" 
            },
            "args": [
                "--model-config",
                "./configs/model/InfinityGAN-IOF.yaml",
                "--test-config",
                "./configs/test/outpaint_with_fused_gen_256x256.yaml",
                "--inv-records",
                "./logs/InfinityGAN-IOF/test/inversion_256x256_L2R/stats/000002.pkl",
                // "./logs/InfinityGAN-IOF/test/inversion_IOF246_256x1280L_256x128/stats/000000.pkl",
                // "--inv-placements=0.5,0.5",
            ]
        },
        {
            "name": "Test generate",
            "type": "python",
            "request": "launch",
            "program": "test.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "/local/omp/infinityGAN/src",
                "CUDA_VISIBLE_DEVICES": "0" 
            },
            "args": [
                "--model-config",
                "./configs/model/InfinityGAN-IOF.yaml",
                "--test-config",
                "./configs/test/infinite_gen_1024x1024.yaml"
            ]
        },
    ]
}
